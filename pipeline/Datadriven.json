						{
							"name": "Data flow1",
							"type": "ExecuteDataFlow",
							"dependsOn": [],
							"policy": {
								"timeout": "1.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"dataflow": {
									"referenceName": "Parametrised",
									"type": "DataFlowReference",
									"parameters": {
										"year": {
											"value": "@item().Year",
											"type": "Expression"
										}
									},
									"datasetParameters": {
										"sink1": {
											"FileName": "test",
											"Directory": "temp"
										}
									}
								},
								"compute": {
									"coreCount": 8,
									"computeType": "General"
								},
								"traceLevel": "Fine"
							}
						}
					]
				}
			}
